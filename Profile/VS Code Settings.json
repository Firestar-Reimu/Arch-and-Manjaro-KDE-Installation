{
    "editor.fontFamily": "JetBrains Mono, Hack, SF Mono, Consolas, monospace",
    "editor.fontLigatures": false,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.renderWhitespace": "boundary",
    "editor.wordWrap": "on",
    "editor.tokenColorCustomizations": {
        "comments": {
            "fontStyle": "bold",
            "foreground": "#AEAEAE"
        },
        "keywords": {
            "fontStyle": "bold",
            "foreground": "#FF791B"
        },
        "functions": {
            "fontStyle": "bold",
            "foreground": "#EE55B7"
        },
        "variables": "#2AA4DB",
        "strings": "#FF7777",
        "numbers": "#A530E0",
        "textMateRules": [
            {
                "scope": [
                    "meta.function-call.generic.python"
                ],
                "settings": {
                    "foreground": "#F43232"
                }
            },
            {
                "scope": [
                    "constant.other.general.math.tex"
                ],
                "settings": {
                    "foreground": "#00D29E"
                }
            },
            {
                "scope": [
                    "keyword.control.cite.latex",
                    "keyword.control.ref.latex"
                ],
                "settings": {
                    "foreground": "#CFBA0F",
                    "fontStyle": "bold"
                }
            }
        ]
    },
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.includeComments": false,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "workbench.colorCustomizations": {
        "editor.selectionHighlightBackground": "#B2FFDD44",
        "editor.wordHighlightBackground": "#74F46644",
        "editor.findMatchBackground":"#A0FFF9",
        "editor.findMatchHighlightBackground":"#FF51C444",
        "editorBracketHighlight.foreground1": "#F8B500",
        "editorBracketHighlight.foreground2": "#5383C3",
        "editorBracketHighlight.foreground3": "#68BE8D",
        "editorBracketHighlight.foreground4": "#BA2636",
        "editorBracketHighlight.foreground5": "#E7609E",
        "editorBracketHighlight.foreground6": "#C8C2C6",
        "editorBracketHighlight.unexpectedBracket.foreground": "#FF3535"
    },
    "workbench.colorTheme": "Default Light Modern",
    "update.showReleaseNotes": false,
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "args": [
                "-NoLogo"
            ],
            "overrideName": true
        }
    },
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.inheritEnv": false,
    "debug.onTaskErrors": "showErrors",
    "explorer.confirmDelete": false,
    "security.workspace.trust.untrustedFiles": "open",
    "search.useReplacePreview": false,
    "files.autoSave": "onFocusChange",
    "files.autoSaveDelay": 0,
    "files.associations": {
        "*.json": "jsonc"
    },
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.autoGuessEncoding": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "git.openRepositoryInParentFolders": "always",
    "markdown.preview.fontFamily": "'Source Han Sans CN', 'Source Sans', sans-serif",
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-lualatex",
                "-bibtex",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "--shell-escape",
                "%DOCFILE%"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        }
    ],
    "latex-workshop.latex.clean.fileTypes": [
        "*.acn",
        "*.acr",
        "*.alg",
        "*.bbl",
        "*.bcf",
        "*.blg",
        "*.fls",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.idx",
        "*.ind",
        "*.ist",
        "*.lof",
        "*.log",
        "*.lot",
        "*.nav",
        "*.out",
        "*.snm",
        "*.toc",
        "*.run.xml"
    ],
    "latex-workshop.message.error.show": false,
    "latex-workshop.message.warning.show": false,
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.intellisense.argumentHint.enabled": false,
    "python.defaultInterpreterPath": "/home/firestar/.miniconda/bin/python",
    "python.analysis.extraPaths": [
        "/home/firestar/.miniconda/bin"
    ]
}
